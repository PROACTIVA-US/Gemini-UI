{
  "name": "gemini-oauth-automation",
  "version": "2.0.0",
  "description": "Autonomous OAuth testing with Gemini Computer Use",
  "main": "src/orchestrator.js",
  "type": "commonjs",
  "scripts": {
    "test": "node src/orchestrator.js --provider github --debug",
    "test:all": "node src/orchestrator.js --all",
    "test:email": "node src/orchestrator.js --provider email",
    "test:github": "node src/orchestrator.js --provider github",
    "test:google": "node src/orchestrator.js --provider google",
    "scenario": "node src/scenario-runner.js",
    "scenario:all": "node src/scenario-runner.js --all",
    "scenario:form": "node src/scenario-runner.js --scenario form-validation",
    "scenario:nav": "node src/scenario-runner.js --scenario navigation-test",
    "scenario:search": "node src/scenario-runner.js --scenario search-functionality",
    "scenario:checkout": "node src/scenario-runner.js --scenario checkout-flow",
    "scenario:responsive": "node src/scenario-runner.js --scenario responsive-design",
    "scenario:a11y": "node src/scenario-runner.js --scenario accessibility-test",
    "scenario:perf": "node src/scenario-runner.js --scenario performance-test",
    "fix:analyze": "node scripts/fix-analyzer.js --interactive",
    "fix:from-log": "node scripts/fix-analyzer.js --from-log",
    "fix:auto": "node scripts/fix-analyzer.js --auto-apply",
    "rotate:secrets": "node src/secret-rotation.js",
    "rotate:dry-run": "node src/secret-rotation.js --dry-run",
    "rotate:stripe": "node src/secret-rotation.js --service stripe",
    "rotate:resend": "node src/secret-rotation.js --service resend",
    "rotate:github": "node src/secret-rotation.js --service github-oauth",
    "rotate:google": "node src/secret-rotation.js --service google-oauth",
    "rotate:supabase": "node src/secret-rotation.js --service supabase",
    "rotate:auto": "node src/secret-rotation.js --auto-approve --test-after"
  },
  "keywords": [
    "gemini",
    "oauth",
    "testing",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.28.0",
    "@google/generative-ai": "^0.24.1",
    "@vercel/client": "^17.1.4",
    "dotenv": "^17.2.3",
    "har-validator": "^5.1.5",
    "playwright": "^1.56.0",
    "simple-git": "^3.20.0"
  }
}
